{
  "kind": "collectionType",
  "collectionName": "product_keys",
  "info": {
    "singularName": "product-key",
    "pluralName": "product-keys",
    "displayName": "product-key",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "key": {
      "type": "string"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "product_keys"
    },
    "key_status": {
      "type": "enumeration",
      "enum": [
        "available",
        "reserved",
        "sold"
      ]
    },
    "sold_at": {
      "type": "datetime"
    },
    "is_used": {
      "type": "boolean",
      "default": false
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "product_keys"
    }
  }
}
